<h1 class="page-header">新建专题</h1>

<%= render "form" %>

<%= render partial: "edit_template", collection: @subject.templates %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">首页头部</h3>
  </div>

  <div class="panel-body">
    <div id="homepage_header" class="source-editor">
    </div>
  </div>
  <div class="panel-footer">
    <%#= f.submit :save, class: 'btn btn-primary' %>
    <%= link_to :save, '#', class: 'btn btn-primary' %>
    <%= link_to :Preview, '#',  class: 'btn btn-default' %>
  </div>
</div>


<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">专题主页</h3>
  </div>

  <div class="panel-body">
    <div id="homepage_header" class="source-editor">
    </div>
  </div>
  <div class="panel-footer">
    <%#= f.submit :save, class: 'btn btn-primary' %>
  </div>
</div>


