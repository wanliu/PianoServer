<% if current_user.state.nil? or current_user.state == "select" %>
  <%= render "after_registers/distributor/step1" %>
<% elsif current_user.state == "industry" %>
  <%= render "after_registers/distributor/step2" %>
<% elsif current_user.state == "shop" %>
  <%= render "after_registers/distributor/step3" %>
<% elsif current_user.state == "category" %>
  <%= render "after_registers/distributor/step4" %>
<% elsif current_user.state == "brand" %>
  <% redirect_to root_path %>
<% end %>
