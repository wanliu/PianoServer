(function() {
  var html = "<%= j(render 'orders/partials/address.html.erb') %>";
  $(".default-chosen-location").replaceWith(html);
  $('#order_address_id').val(<%= @location.id %>);

  var formData = $('#new_order').serialize()
  $.post('/orders/express_fee?' + formData)
    .done(function(html) {
      $('.confirmation-total').html(html);
    })

  window.history.back();
})();