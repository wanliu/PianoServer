default: &default
  appid: "aaaaaaaa"
  secret: "aaaaaaa"
  token: "aaaaaaaaa"
  access_token: "/var/tmp/wechat_access_token"
  encrypt_mode: false # if true must fill encoding_aes_key
  encoding_aes_key:  "my_encoding_aes_key"
  jsapi_ticket: "/var/tmp/wechat_jsapi_ticket"

production:
  timeout: 30,
  skip_verify_ssl: true

development:
  <<: *default

test:
  <<: *default